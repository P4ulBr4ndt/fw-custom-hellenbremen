{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "EFI_CAN_SUPPORT=1",
                "EFI_PROD_CODE=1",
                "EFI_HD_ACR=1",
                "EFI_BOOST_CONTROL=0",
                "EFI_UNIT_TEST=0",
				"HAL_USE_ADC=1",
				"STM32_ADC_USE_ADC3=1",
				"EFI_SOFTWARE_KNOCK=1",
				"ADC_MUX_PIN=1",
				"EFI_BLUETOOTH_SETUP=1",
				"EFI_CONSOLE_TX_BRAIN_PIN=Gpio::D5",
				"EFI_CONSOLE_RX_BRAIN_PIN=Gpio::D6",
				"TS_PRIMARY_UxART_PORT=UARTD2",
				"STM32_UART_USE_USART2=1",

            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++14",
            "intelliSenseMode": "macos-clang-arm64"
        }
    ],
    "version": 4
}