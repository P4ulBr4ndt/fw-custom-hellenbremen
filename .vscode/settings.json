		"**/.dep": true,
		"**/build/lst": true,
		"**/build/obj": true,
		"hardware/": true
	},
	"search.exclude": {
		"**/.dep": true,
		"**/build/lst": true,
		"**/build/obj": true,
		"hardware/": true,
		"firmware/ChibiOS/demos": true
	},
